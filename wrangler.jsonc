{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "plarail-sns",
  "compatibility_date": "2025-11-29",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "plarail-sns-production",
      "database_id": "00000000-0000-0000-0000-000000000000"
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "plarail-images"
    }
  ]
}